{
  "globals": {
    "expect": true,
    "describe":true,
    "shallow":true,
    "spy":true,
    "it":true,
    "mount":true
  },
  "extends": "@folio/eslint-config-stripes",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": true
    }],
    "no-console": ["error", { "allow": ["warn", "error"] }]
  }
}
